@model WenShenERP.Models.City

@{
    ViewBag.Title = "Create";
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
<table width="98%" border="0" cellspacing="0" cellpadding="0" id="table_main_title">
        <tr>
        <td style="padding-left:13px;">添加城市</td>
        <td width="50"><input type="submit" class="buttonStyle1" value="保存" /></td>
        <td width="50"><input type="button" class="buttonStyle1" value="返回" onClick="javascript: history.go(-1)"/></td></tr>

    </table>
<table width="98%" border="0" cellspacing="0" cellpadding="0" class="table_form" style="margin-top:15px;">
        <tr><td width="65">
            @Html.LabelFor(model => model.CityName)
        </td>
		<td>
        <td>
            @Html.EditorFor(model => model.CityName)
            @Html.ValidationMessageFor(model => model.CityName)
        </td>
		</tr>

        <tr><td>
            @Html.LabelFor(model => model.PyCode)
        </td>
		<td>
        <td>
            @Html.EditorFor(model => model.PyCode)
            @Html.ValidationMessageFor(model => model.PyCode)
        </td>
		</tr>

        <tr><td>
            @Html.LabelFor(model => model.ProvinceID)
        </td>
		<td>
        <td>
            @Html.DropDownListFor(model => model.ProvinceID,ViewData["List"] as SelectList,"请选择")

        </td>
		</tr>

        <tr><td>
            @Html.LabelFor(model => model.Postcode)
        </td>
		<td>
        <td>
            @Html.EditorFor(model => model.Postcode)
            @Html.ValidationMessageFor(model => model.Postcode)
        </td>
		</tr>

        <tr><td>
            @Html.LabelFor(model => model.AreaCode)
        </td>
		<td>
        <td>
            @Html.EditorFor(model => model.AreaCode)
            @Html.ValidationMessageFor(model => model.AreaCode)
        </td>
		</tr>

 </table>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
